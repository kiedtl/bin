#!/usr/bin/csi -s
;
; 256 -- print colors in the 256 palette
; implemented in Scheme
; (c) KiÃ«d Llaentenn
; See the LICENSE for more information

(define e #\x1B)
(define (tostr i) (number->string i))

(define (colors i)
  (display
    (string-append (tostr i) "\u001B[4D\u001B[4C\u001B[48;5;"
                   (tostr i) "m\u001B[K\u001B[m\n"))
  (if (< i 255) (colors (+ 1 i))))

(colors 0)
